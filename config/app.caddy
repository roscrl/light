www.CHANGE_ME.com CHANGE_ME.com {
    tls /etc/ssl/certs/app.pem /etc/ssl/private/app.pem

    reverse_proxy localhost:8080 {
        flush_interval -1
    }

    encode gzip

    log {
        output file /var/log/caddy/app.com.access.log {
            roll_size 1GB
            roll_keep 5
            roll_keep_for 720h
        }
    }

    request_body {
      max_size 5KB
    }
}